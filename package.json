{
  "name": "get-url-origin",
  "version": "1.0.0",
  "description": "Get origin from url string in Node.js",
  "keywords": [
    "node.js",
    "origin",
    "url"
  ],
  "homepage": "https://github.com/azu/node-get-url-origin",
  "bugs": {
    "url": "https://github.com/azu/node-get-url-origin/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/node-get-url-origin.js",
  "types": "lib/node-get-url-origin.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/node-get-url-origin.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.46",
    "@types/node": "^7.0.52",
    "cross-env": "^5.1.3",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^4.1.0",
    "prettier": "^1.9.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4
  }
}